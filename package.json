{
  "name": "rollup-starter-app",
  "type": "module",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-sucrase": "^4.0.4",
    "@rollup/plugin-html": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^14.0.1"
  },
  "dependencies": {
    "date-fns": "^2.29.3",
    "peerjs": "^1.4.7"
  },
  "scripts": {
    "build": "rm -rf public/* && rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public",
    "peer": "pnpx peer --port 9000 --key peerjs --path /myapp",
    "gh": "npm run build && cp public/index.html public/404.html && npx -y gh-pages -d public -b gh-pages"
  }
}
