{
  "name": "rollup-starter-app",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^14.0.0",
    "@rollup/plugin-sucrase": "^4.0.4",
    "@web/rollup-plugin-html": "^1.11.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.36.2",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^14.0.0"
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "peerjs": "^1.4.7"
  },
  "scripts": {
    "build": "rm -rf public/* && rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public",
    "gh": "npm run build && cp public/index.html public/404.html && npx -y gh-pages -d public -b gh-pages"
  }
}
